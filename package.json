{
  "name": "@blackglory/refile-js",
  "version": "0.8.0",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "main": "lib/es2018/index.js",
  "types": "lib/es2018/index.d.ts",
  "browser": {
    "./lib/es2015/utils/get-hash-info.js": "./lib/es2015/utils/get-hash-info.browser.js",
    "./lib/es2018/utils/get-hash-info.js": "./lib/es2018/utils/get-hash-info.browser.js",
    "./lib/es2015/utils/get-file.js": "./lib/es2015/utils/get-file.browser.js",
    "./lib/es2018/utils/get-file.js": "./lib/es2018/utils/get-file.browser.js"
  },
  "repository": "git@github.com:BlackGlory/refile-js.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --no-cache --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s prepare clean build bundle",
    "clean": "run-p clean:*",
    "clean:build": "rimraf lib",
    "clean:bundle": "rimraf dist",
    "build": "run-p build:*",
    "build:es2015": "tsc --project tsconfig.build.json --module commonjs --target es2015 --outDir lib/es2015",
    "build:es2018": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib/es2018",
    "bundle": "rollup --config rollup.config.js",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-matchers": "^0.2.1",
    "@blackglory/log": "^0.1.0",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/crypto-js": "^4.1.0",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "fastify": "^3.27.1",
    "fastify-multipart": "^5.3.0",
    "husky": "4",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.3",
    "ts-patch": "^2.0.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "@blackglory/errors": "^2.2.0",
    "@blackglory/http-status": "^1.0.1",
    "@blackglory/types": "^0.6.5",
    "extra-abort": "^0.1.1",
    "extra-fetch": "^2.0.0",
    "extra-request": "^5.0.0",
    "extra-response": "^0.3.9",
    "iterable-operator": "^0.14.5",
    "justypes": "^0.1.6",
    "split-hash": "^0.1.6"
  }
}
