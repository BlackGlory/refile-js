{
  "name": "@blackglory/refile-js",
  "version": "0.10.0",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "dist"
  ],
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "browser": {
    "./lib/utils/get-hash-info.js": "./lib/utils/get-hash-info.browser.js",
    "./lib/utils/get-file.js": "./lib/utils/get-file.browser.js"
  },
